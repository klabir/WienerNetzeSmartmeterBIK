meta {
  name: auth_token
  type: http
  seq: 4
}

post {
  url: {{auth_url}}/token
  body: form
}

headers {
  Content-Type: application/x-www-form-urlencoded
}

body:form {
  grant_type: authorization_code
  client_id: {{client_id}}
  redirect_uri: {{redirect_uri}}
  code: {{auth_code}}
  code_verifier: {{code_verifier}}
}
